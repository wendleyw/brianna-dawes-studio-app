{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://miro.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://miro.com https://*.miro.com; connect-src 'self' https://*.supabase.co wss://*.supabase.co https://miro.com https://*.miro.com;"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|.*\\.html$).*)",
      "destination": "/index.html"
    }
  ]
}
